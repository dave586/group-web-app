@{
    ViewBag.Title = Questionnaire.Resources.Home.Views.Resource.PageTitleRestart;
    Layout = "~/Views/Shared/_layout.cshtml";
}

<div class="row section">
    <div class="col-sm-12">
        <div class="row paragraph">
            <div class="col-sm-2"></div>
            <div class="col-sm-8">
                <p>@Questionnaire.Resources.Home.Views.Resource.PageDescriptionRestart</p>
            </div>
            <div class="col-sm-2"></div>
        </div>
        
        <div class="row">
            <div class="col-sm-3"></div>
            <div class="col-sm-6">
                <a class="btn" href="@Url.Action("Index", "Home")" title="@Questionnaire.Resources.Home.Views.Resource.BtnRestartTests">@Questionnaire.Resources.Home.Views.Resource.BtnRestartTests</a>
            </div>
            <div class="col-sm-3"></div>
        </div>
        <div class="row">
            <div class="col-sm-3"></div>
            <div class="col-sm-6">
                <a class="btn" href="@Url.Action("ValidateClientID", "Home")" title="@Questionnaire.Resources.Home.Views.Resource.BtnClientOverride">@Questionnaire.Resources.Home.Views.Resource.BtnClientOverride</a>
            </div>
            <div class="col-sm-3"></div>
        </div>
    </div>
